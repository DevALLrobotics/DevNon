{
  "tests": [
    {
      "name": "empty_input",
      "input": {
        "nums": []
      },
      "expected": 0
    },
    {
      "name": "single_element",
      "input": {
        "nums": [42]
      },
      "expected": 42
    },
    {
      "name": "large_duplicates",
      "input": {
        "nums": [5, 5, 7, 7, 7, 9, 9, 11, 13, 13, 15]
      },
      "expected": 26
    },
    {
      "name": "multiple_unique_spread",
      "input": {
        "nums": [10, -1, 10, 2, 2, 3, 4, 4, 5, 6, 6]
      },
      "expected": 7
    },
    {
      "name": "wide_range_values",
      "input": {
        "nums": [-10000, -9999, -10000, 5000, 5000, 123]
      },
      "expected": -9876
    }
  ]
}
